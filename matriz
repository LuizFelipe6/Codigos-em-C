#include<stdio.h>
#include<conio.h>
int main (){
 int n[5][5], m, b;

for (m = 0; m < 5; m++){
    for (b = 0; b < 5; b++){
	  printf ("digite um valor %d coluna %d:", m+1, b+1);
	  scanf ("%d", &n[m][b]);
}
    }
for(m=0; m<5;m++){
    for (b=0;b<5;b++){
printf ("\n[%i][%i]",n[m][b]);
}
    }

return 0;
}
